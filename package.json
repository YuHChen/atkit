{
  "name": "atkit",
  "version": "0.1.0",
  "homepage": "https://yuhchen.github.io/atkit",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-bootstrap": "^0.24.5",
    "bootstrap": "^4.5.0",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm run check && react-scripts start",
    "build": "npm run build:prepare && react-scripts build",
    "build:prepare": "rm -rf build/ && npm run lint:fix && npm run format:fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check": "npm run lint && npm run format",
    "lint": "eslint --color \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:fix": "eslint --color --fix \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
