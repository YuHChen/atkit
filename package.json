{
  "name": "atkit",
  "version": "0.1.0",
  "homepage": "https://yuhchen.github.io/atkit",
  "private": true,
  "dependencies": {
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-bootstrap": "^0.24.5",
    "bootstrap": "^4.5.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.1.5",
    "@types/enzyme": "^3.10.6",
    "@types/jest": "^26.0.10",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.5.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.1"
  },
  "scripts": {
    "clean": "rm -rf build/",
    "check": "npm run style && npm run test:coverage -- --watchAll=false",
    "style": "npm run lint && npm run format",
    "style:fix": "npm run lint:fix && npm run format:fix",
    "lint": "eslint --color \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:fix": "eslint --color --fix \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:fix": "react-scripts test --updateSnapshot --watchAll=false",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
