{
  "name": "atkit",
  "version": "0.1.0",
  "type": "module",
  "homepage": "https://yuhchen.github.io/atkit",
  "private": true,
  "dependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-bootstrap": "^0.26.6",
    "bootstrap": "^5.3.3",
    "highlight.js": "^11.10.0",
    "node-sass": "^9.0.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18.3.1",
    "react-router-bootstrap": "^0.26.3",
    "react-router-dom": "^6.26.2",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "gh-pages": "^6.1.1",
    "jsdom": "^25.0.1",
    "msw": "^2.4.9",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "vite": "^5.4.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.3"
  },
  "scripts": {
    "clean": "rm -rf {build,coverage,node_modules}",
    "check": "npm run style && npm run test:coverage",
    "style": "npm run lint && npm run format",
    "style:fix": "npm run lint:fix && npm run format:fix",
    "lint": "eslint --color \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:fix": "eslint --color --fix \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:fix": "vitest run --update",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
